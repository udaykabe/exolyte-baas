{
  "name": "Parse Server Example",
  "description": "An example Parse API server using the parse-server module",
  "repository": "https://github.com/ParsePlatform/parse-server-example",
  "logo": "https://avatars0.githubusercontent.com/u/1294580?v=3&s=200",
  "keywords": ["node", "express", "parse"],
  "env": {
    "PARSE_MOUNT": {
      "description": "Configure Parse API route.",
      "value": "/parse"
    },
    "APP_ID": {
      "description": "A unique identifier for your app.",
      "value": "myAppId"
    },
    "MASTER_KEY": {
      "description": "A key that overrides all permissions. Keep this secret.",
      "value": "myMasterKey"
    },
    "SERVER_URL": {
      "description": "URL to connect to your Heroku instance (update with your app's name + PARSE_MOUNT)",
      "value": "https://yourappname.herokuapp.com/parse"
    },
    "MAILGUN_FROM_ADDRESS": {
      "description": "From: address for emails from mailgun.com",
      "value": "no-reply@mailgun.example.com"
    },
    "MAILGUN_DOMAIN": {
      "description": "email domain from mailgun.com",
      "value": "mailgun.example.com"
    },
    "MAILGUN_API_KEY": {
      "description": "mailgun.com API Key",
      "value": "xxxxxxxxxxxx"
    },
    "TWILIO_AUTH_TOKEN": {
      "description": "Auth token for twilio.com",
      "value": "xxxxxxxxxxxx"
    },
    "TWILIO_ACCOUNT_SID": {
      "description": "twilio.com Account SID",
      "value": "xxxxxxxxxxxx"
    },
    "TWILIO_TELEPHONE_NUMBER": {
      "description": "twilio.com sending telephone number",
      "value": "+1xxx5551212"
    }
  },
  "image": "heroku/nodejs",
  "addons": ["mongolab"]
}
